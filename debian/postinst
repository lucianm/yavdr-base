/usr/bin/process-template --owner 0 --group 0 --mode 0644 /etc/init.d/yavdr-shutdown
/usr/bin/process-template --owner 0 --group 0 --mode 0644 /etc/init/usb-wakeup.conf
/usr/bin/process-template --owner 0 --group 0 --mode 0644 /etc/initramfs-tools/conf.d/splash
